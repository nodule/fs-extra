{
  "name": "copy",
  "ns": "fs",
  "title": "Copy File",
  "description": "Copies a file",
  "phrases": {
    "active": "Copying file from {{input.from}} to {{input.to}}"
  },
  "require": {
    "fs.extra": "1.x.x"
  },
  "ports": {
    "input": {
      "from": {
        "title": "From Filename",
        "type": "string",
        "required": true
      },
      "to": {
        "title": "To Filename",
        "type": "string",
        "required": true
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      }
    }
  }
}
